  "FileBox": {
    "component_name": "FileBox",
    "name": "FileBox",
    "label": "文件上传框",
    "inline": false,
    "conf": {
      "action": "/upload/file",
      "limit": 5,
      "tip": "上传文件限制不超过2M",
      "auto-upload": false
    }
  },
  "MiniFormBox": {
    "component_name": "MiniFormBox",
