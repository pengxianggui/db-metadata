  "ImgBox": {
    "component_name": "ImgBox",
    "name": "ImgBox",
    "label": "图片上传框",
    "inline": false,
    "conf": {
      "action": "/upload/img",
      "drag": false,
      "list-type": "picture-card",
      "auto-upload": false,
      "accept": "image"
    }
